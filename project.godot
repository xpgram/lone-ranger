; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[addons]

instance_dock/scene_data_file="res://InstanceDockSceneData.txt"
instance_dock/preview_resolution=Vector2i(64, 64)

[application]

config/name="Lone Ranger"
config/features=PackedStringArray("4.5", "Forward Plus")
config/icon="res://icon.svg"

[autoload]

Constants="*res://utilities/autoload/constants.gd"
Enums="*res://utilities/autoload/enums.gd"
Events="*res://utilities/autoload/events.gd"
InputFocus="*res://utilities/autoload/input_focus.gd"
Grid="*res://utilities/autoload/grid.gd"
BetterTerrain="*res://addons/better-terrain/BetterTerrain.gd"

[display]

window/size/viewport_width=720
window/size/viewport_height=480
mouse_cursor/tooltip_position_offset=Vector2(256, -16)

[editor_plugins]

enabled=PackedStringArray("res://addons/InstanceDock/plugin.cfg", "res://addons/better-terrain/plugin.cfg")

[file_customization]

folder_colors={
"res://assets/": "green",
"res://entities/": "pink",
"res://stages/": "purple",
"res://utilities/": "yellow"
}

[global_group]

terrain_data="World terrain data in the form of special TileMapLayers"
enemy_entities="Enemy GridEntities"
interactible_entities="Interactible or inert GridEntities (e.g. chests, warp zones, etc.)"
npc_entities="NPC GridEntities, or allied non-players"
player_entities="Player GridEntity"

[gui]

timers/tooltip_delay_sec=1.5
timers/tooltip_delay_sec.editor_hint=1.5

